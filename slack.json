{
  "display_information": {
      "name": "Security Bot",
      "description": "A security bot to inform users of suspicious logins",
      "background_color": "#000000"
  },
  "features": {
      "bot_user": {
          "display_name": "Security Bot",
          "always_online": true
      }
  },
  "oauth_config": {
      "scopes": {
          "bot": [
              "incoming-webhook",
              "im:write",
              "users:read.email",
              "users:read"
          ]
      }
  },
  "settings": {
      "interactivity": {
          "is_enabled": true,
          "request_url": "http://hooks.slack.com/services/T00000000/B00000000/XXXXXXXXXXXXXXXXXXXXXXXX"
      },
      "org_deploy_enabled": true,
      "socket_mode_enabled": false,
      "token_rotation_enabled": false
  }
}